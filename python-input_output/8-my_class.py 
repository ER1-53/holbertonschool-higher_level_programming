#!/usr/bin/python3
&quot;&quot;&quot; My class module
&quot;&quot;&quot;

class MyClass:
    &quot;&quot;&quot; My class
    &quot;&quot;&quot;

    def __init__(self, name):
        self.name = name
        self.number = 0

    def __str__(self):
        return &quot;[MyClass] {} - {:d}&quot;.format(self.name, self.number)

